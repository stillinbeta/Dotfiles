[user]
	name = Liz Frost
	email = lfrost@pagerduty.com
[core]
	excludesfile = /home/liz/.gitignore_global
  editor = emacsclient
[color]
	ui = auto
[push]
	default = simple
[magithub]
	online = false
[magithub "status"]
	includeStatusHeader = false
	includePullRequestsSection = false
	includeIssuesSection = false
[credential]
	helper = store
[url "git@github.com:pagerduty"]
	insteadOf = https://github.com/PagerDuty
	insteadOf = https://github.com/pagerduty
[init]
	defaultBranch = main

